{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["next/core-web-vitals", "prettier", "react-app"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "plugins": ["prettier", "react"],
  "rules": {
    "@next/next/no-img-element": "off",
    "jsx-a11y/anchor-is-valid": "off",
    "prettier/prettier": "error"
  }
}
